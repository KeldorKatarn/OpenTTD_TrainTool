    <Style x:Key="PaddedAxisStyle"
           TargetType="chartingToolkit:AxisLabel">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="chartingToolkit:AxisLabel">
                    <TextBlock Text="{TemplateBinding FormattedContent}"
                               FontSize="12"
                               Margin="5,0"
                               ClipToBounds="False"
                               TextTrimming="CharacterEllipsis" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RotateAxisStyle"
           TargetType="chartingToolkit:AxisLabel">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="chartingToolkit:AxisLabel">
                    <TextBlock Text="{TemplateBinding FormattedContent}"
                               FontSize="10"
                               Margin="0,5"
                               ClipToBounds="False"
                               TextTrimming="CharacterEllipsis">
                        <TextBlock.LayoutTransform>
                            <RotateTransform Angle="-90" />
                        </TextBlock.LayoutTransform>
                    </TextBlock>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

